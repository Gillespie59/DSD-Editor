###################################################################
#               Data Structure Definition for Esane               #
#                  (base part, without measures)                  #
###################################################################

<http://id.insee.fr/meta/dsd/esane>
        a             qb:DataStructureDefinition ;
    dcterms:identifier       "esane";
    skos:prefLabel      "Esane";
		dc:description		"DSD Esane v0.1";
        qb:component  [ a                       qb:ComponentSpecification ;
                        qb:attribute            iatt:unitMult ;
                        qb:componentAttachment  qb:DataSet
                      ] ;
        qb:component  [ a                       qb:ComponentSpecification ;
                        qb:attribute            iatt:decimals ;
                        qb:componentAttachment  qb:DataSet
                      ] ;
        qb:component  [ a                       qb:ComponentSpecification ;
                        qb:attribute            iatt:unitMeasure;
                        qb:componentAttachment  qb:Observation
                      ] ;
        qb:component  [ a                       qb:ComponentSpecification ;
                        qb:attribute            iatt:confStatus ;
                        qb:componentAttachment  qb:Observation
                      ] ;

    qb:component  [ a                       qb:ComponentSpecification ;
                        qb:attribute            iatt:dataRev ;
                        qb:componentAttachment  qb:Observation
                      ] ;


        qb:component  [ a                       qb:ComponentSpecification ;
                        qb:dimension            sdmx-dimension:timePeriod
            ] ;

        qb:component  [ a                       qb:ComponentSpecification ;
                        qb:dimension            idim:nafRev2
            ] ;

        qb:component  [ a                       qb:ComponentSpecification ;
                        qb:dimension            idim:trancheEffectif
            ] ;

        qb:component  [ a                       qb:ComponentSpecification ;
                        qb:dimension            idim:freq
            ] ;

    qb:component  [ a                       qb:ComponentSpecification ;
                        qb:dimension            qb:measureType;
            ] .
